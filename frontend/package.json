{
  "name": "radish_frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link --project-ref swdxtdivwobldhezckdn",
    "db:migrate:new": " supabase migration new",
    "db:migrate:up": " supabase migration up --linked",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node database/seedSubscriptions.js",
    "supabase:types": "npx supabase gen types --lang=typescript --project-id swdxtdivwobldhezckdn --schema public > database/types.ts"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@supabase/supabase-js": "^2.46.1",
    "@tanstack/vue-table": "^8.20.5",
    "@vueuse/core": "^11.2.0",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "iconify-icon": "^2.1.0",
    "pinia": "^2.2.4",
    "radix-vue": "^1.9.8",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.1.0",
    "@playwright/test": "^1.48.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-plugin-playwright": "^1.7.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "supabase": "^1.207.9",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.5.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.8",
    "vitest": "^2.1.2",
    "vue-tsc": "^2.1.6"
  }
